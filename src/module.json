{
  "id": "cloudseed",
  "name": "CloudSeed",
  "version": "1.0.0",
  "description": "Algorithmic reverb - exact port of CloudSeedCore by Ghost Note Audio",
  "author": "Ghost Note Audio / Charles Vestal",
  "dsp": "cloudseed.so",
  "api_version": 1,
  "capabilities": {
    "chainable": true,
    "component_type": "audio_fx",
    "chain_params": [
      {"key": "mix", "name": "Mix", "type": "float", "min": 0.0, "max": 1.0, "default": 0.3, "step": 0.01},
      {"key": "decay", "name": "Decay", "type": "float", "min": 0.0, "max": 1.0, "default": 0.5, "step": 0.01},
      {"key": "size", "name": "Size", "type": "float", "min": 0.0, "max": 1.0, "default": 0.5, "step": 0.01},
      {"key": "predelay", "name": "Pre-Delay", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.01},
      {"key": "diffusion", "name": "Diffusion", "type": "float", "min": 0.0, "max": 1.0, "default": 0.7, "step": 0.01},
      {"key": "low_cut", "name": "Low Cut", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.01},
      {"key": "high_cut", "name": "High Cut", "type": "float", "min": 0.0, "max": 1.0, "default": 1.0, "step": 0.01},
      {"key": "mod_amount", "name": "Mod Amount", "type": "float", "min": 0.0, "max": 1.0, "default": 0.3, "step": 0.01},
      {"key": "mod_rate", "name": "Mod Rate", "type": "float", "min": 0.0, "max": 1.0, "default": 0.3, "step": 0.01},
      {"key": "cross_seed", "name": "Stereo Width", "type": "float", "min": 0.0, "max": 1.0, "default": 0.5, "step": 0.01}
    ]
  }
}
